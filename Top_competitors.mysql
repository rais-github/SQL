SELECT H.hacker_id, H.name FROM Hackers H JOIN Submissions S ON H.hacker_id = S.hacker_id JOIN Challenges C ON C.challenge_id = S.challenge_id JOIN Difficulty D ON D.difficulty_level = C.difficulty_level AND S.score = D.score GROUP BY H.hacker_id, H.name HAVING COUNT(H.hacker_id) > 1 ORDER BY COUNT(H.hacker_id) DESC, H.hacker_id;
